{
  "expo": {
    "name": "Ampli",
    "slug": "ampli-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ampli-app-icon.svg",
    "scheme": "amplifrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#232323",
        "foregroundImage": "./assets/ampli-app-icon.svg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/ampli-app-icon.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/ampli-logo-white.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#B794F6",
          "dark": {
            "backgroundColor": "#B794F6"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
