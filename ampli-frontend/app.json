{
  "expo": {
    "name": "Ampli",
    "slug": "ampli-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "amplifrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emilianoferreyra.amplifrontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.emilianoferreyra.amplifrontend",
      "permissions": ["android.permission.FOREGROUND_SERVICE"]
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5b216212-6233-420a-adc4-97012e8729e4"
      }
    }
  }
}
