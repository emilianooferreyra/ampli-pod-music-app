{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@views/*": ["src/views/*"],
      "lib/*": ["./src/lib/*"],
      "@ui/*": ["src/ui/*"],
      "#/*": ["src/*"]
    }
  }
}
